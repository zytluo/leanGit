<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
  <form-beans >
     <form-bean name="employeeform" type="com.luo.web.from.EmployeeForm">
     </form-bean>
  </form-beans>
  <action-mappings >
   <action
      path="/login"
      parameter="flag"
      name="employeeform"
      type="com.luo.web.action.loginAction">
      <forward name="err" path="/WEB-INF/web/error.jsp" />
      <forward name="ok"    path="/WEB-INF/web/mainFrame.jsp" />
    </action>
  </action-mappings>
</struts-config>

